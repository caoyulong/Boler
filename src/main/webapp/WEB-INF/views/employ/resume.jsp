<div class="col-md-6 col-md-offset-3">
  <h1>在线简历</h1>
  <div>
    <form role="form" id="resumeForm">
      <div class="form-group">
        <label for="name">名 称</label>
        <input id="name" type="text" class="form-control" name="name" placeholder="请输入名称">
      </div>
      <h3>教育经历</h3>
      <div class="form-group">
        <label for="edu_startTime">开始时间</label>
        <input id="edu_startTime" type="date" class="form-control" name="edu_startTime">
      </div>
      <div class="form-group">
        <label for="edu_endTime">结束时间</label>
        <input id="edu_endTime" type="date" class="form-control" name="edu_endTime">
      </div>
      <div class="form-group">
        <label for="school">学 校</label>
        <input id="school" type="text" class="form-control" name="school">
      </div>
      <div class="form-group">
        <label for="degree">学 历</label>
        <select class="form-control" id="degree">
          <option value="1">学 士</option>
          <option value="2">硕 士</option>
          <option value="3">博 士</option>
        </select>
      </div>
      <div class="form-group">
        <label for="major">专 业</label>
        <input id="major" type="text" class="form-control" name="major">
      </div>
      <div class="form-group">
        <label for="course">主要课程</label>
        <input id="course" type="text" class="form-control" name="course">
      </div>
      <h3>工作经历</h3>
      <div class="form-group">
        <label for="work_startTime">开始时间</label>
        <input id="work_startTime" type="date" class="form-control" name="work_startTime">
      </div>
      <div class="form-group">
        <label for="work_endTime">结束时间</label>
        <input id="work_endTime" type="date" class="form-control" name="work_endTime">
      </div>
      <div class="form-group">
        <label for="corporation">公 司</label>
        <input id="corporation" type="text" class="form-control" name="corporation">
      </div>
      <div class="form-group">
        <label for="scale">规 模</label>
        <input id="scale" type="text" class="form-control" name="scale">
      </div>
      <div class="form-group">
        <label for="department">部 门</label>
        <input id="department" type="text" class="form-control" name="department">
      </div>
      <div class="form-group">
        <label for="job">岗 位</label>
        <input id="job" type="text" class="form-control" name="job">
      </div>
      <div class="form-group">
        <label for="detail">工作内容</label>
        <textarea id="detail" class="form-control" name="detail" rows="10" placeholder="请输入工作内容"></textarea>
      </div>
      <h3>项目经历</h3>
      <div class="form-group">
        <label for="proj_startTime">开始时间</label>
        <input id="proj_startTime" type="date" class="form-control" name="proj_startTime">
      </div>
      <div class="form-group">
        <label for="proj_endTime">结束时间</label>
        <input id="proj_endTime" type="date" class="form-control" name="proj_endTime">
      </div>
      <div class="form-group">
        <label for="proj_name">名 称</label>
        <input id="proj_name" type="text" class="form-control" name="proj_name">
      </div>
      <div class="form-group">
        <label for="team_scale">规 模</label>
        <input id="team_scale" type="text" class="form-control" name="team_scale">
      </div>
      <div class="form-group">
        <label for="role">角 色</label>
        <input id="role" type="text" class="form-control" name="role">
      </div>
      <div class="form-group">
        <label for="introduction">项目介绍</label>
        <textarea id="introduction" class="form-control" name="introduction" rows="10" placeholder="请输入项目介绍"></textarea>
      </div>
      <div class="form-group">
        <label for="proj_detail">工作内容</label>
        <textarea id="proj_detail" class="form-control" name="proj_detail" rows="10" placeholder="请输入工作内容"></textarea>
      </div>
    </form>
    <button class="btn btn-default" id="resumeSubmit">确 认</button>
  </div>
</div>